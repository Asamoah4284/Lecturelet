{
  "expo": {
    "name": "LectureLet",
    "slug": "LectureLet",
    "version": "1.0.4",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "icon": "./assets/logo-1.png",
    "splash": {
      "image": "./assets/logo-1.png",
      "resizeMode": "contain",
      "backgroundColor": "#6366f1"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.LectureLet.app",
      "buildNumber": "4",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ],
        "ITSAppUsesNonExemptEncryption": false
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo-1.png",
        "backgroundColor": "#6366f1"
      },
      "package": "com.LectureLet.app",
      "versionCode": 5,
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "com.android.alarm.permission.SET_ALARM"
      ]
    },
    "web": {
      "favicon": "./assets/logo-1.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "color": "#2563eb",
          "sounds": [],
          "mode": "production",
          "iosDisplayInForeground": true,
          "androidMode": "default",
          "androidCollapsedTitle": "#{unread_notifications} new notifications"
        }
      ]
    ],
    "extra": {
      "apiUrl": "https://lecturelet.onrender.com/api",
      "PAYSTACK_PUBLIC_KEY": "pk_test_5fe292bf0df872407adaebe53b5982e173a45f44",
      "eas": {
        "projectId": "ea913b88-c870-4e26-90b7-87661d3890f0"
      }
    },
    "owner": "asamoah4284"
  }
}
