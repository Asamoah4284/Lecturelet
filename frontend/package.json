{
  "name": "LectureLet",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "start:dev-client": "expo start --dev-client --scheme lecturelet",
    "start:tunnel": "expo start --dev-client --tunnel --scheme lecturelet",
    "start:dev": "expo start --dev-client --tunnel --scheme lecturelet",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "update:android": "eas update --platform android",
    "update:ios": "eas update --platform ios",
    "update:all": "eas update --branch production --message \"Update\"",
    "build:android": "eas build --platform android",
    "build:android:apk": "eas build --platform android --profile preview",
    "build:ios": "eas build --platform ios",
    "build:all": "eas build --platform all",
    "build:dev": "eas build --platform android --profile development",
    "build:local:dev": "eas build --platform android --profile development --local",
    "build:local:apk": "eas build --platform android --profile preview --local"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "^8.5.1",
    "@react-native-community/netinfo": "11.4.1",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/stack": "^6.4.1",
    "expo": "~54.0.0",
    "expo-clipboard": "^8.0.8",
    "expo-dev-client": "~6.0.20",
    "expo-document-picker": "~14.0.8",
    "expo-file-system": "~19.0.21",
    "expo-notifications": "~0.28.0",
    "expo-status-bar": "~3.0.8",
    "expo-updates": "~29.0.16",
    "firebase": "^12.8.0",
    "firebase-admin": "^13.6.0",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-paystack-webview": "^5.0.3",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.0"
  },
  "private": true
}
